{
  "name": "vscode-one-dark-theme",
  "displayName": "One dark theme",
  "description": "One dark theme for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "markskelton",
  "author": "Mark Skelton",
  "repository": {
    "type": "git",
    "url": "https://github.com/markypython/vscode-one-dark-theme.git"
  },
  "bugs": {
    "url": "https://github.com/markypython/vscode-one-dark-theme/issues"
  },
  "homepage": "https://github.com/markypython/vscode-one-dark-theme#readme",
  "license": "MIT",
  "keywords": [
    "one-dark",
    "vscode-theme",
    "vscode"
  ],
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "static/icon.png",
  "galleryBanner": {
    "color": "#2d323b",
    "theme": "dark"
  },
  "badges": [
    {
      "href": "https://travis-ci.com/markypython/vscode-one-dark-theme",
      "url": "https://img.shields.io/travis/com/markypython/vscode-one-dark-theme/master.svg",
      "description": "Build status"
    },
    {
      "href": "https://github.com/markypython/vscode-one-dark-theme/releases/latest",
      "url": "https://img.shields.io/github/tag/markypython/vscode-one-dark-theme.svg",
      "description": "GitHub tag"
    },
    {
      "href": "https://marketplace.visualstudio.com/items?itemName=markskelton.one-dark-theme",
      "url": "https://img.shields.io/visual-studio-marketplace/v/markskelton.one-dark-theme",
      "description": "VS Code extension version"
    },
    {
      "href": "https://marketplace.visualstudio.com/items?itemName=markskelton.one-dark-theme",
      "url": "https://img.shields.io/visual-studio-marketplace/d/markskelton.one-dark-theme",
      "description": "VS Code extension downloads"
    }
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "One dark",
        "uiTheme": "vs-dark",
        "path": "./themes/one-dark.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/vscode": "^1.37.0",
    "typescript": "^3.5.3"
  }
}
